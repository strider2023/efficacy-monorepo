{
    "compilerOptions": {
        "esModuleInterop": false,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "noEmit": true,
        "strict": true
    },
    "exclude": [
        "node_modules"
    ]
}